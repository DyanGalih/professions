{
    "name": "webappid/profession",
    "description": "Daftar List Pekerjaan Di Indonesia",
    "license": "MIT",
    "authors": [
        {
            "name": "Dyan Galih",
            "email": "dyan.galih@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "WebAppId\\Content\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebAppId\\Content\\Tests\\": "src/tests"
        }
    },
    "require": {
        "php" : ">=7.1.0",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "7.2.6",
        "squizlabs/php_codesniffer": "3.*",
        "orchestra/testbench": "~3.5"
    },
   "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
       "laravel": {
           "providers": [
               "WebAppId\\Content\\ServiceProvider"
           ],
           "aliases": {
               "Content" : "WebAppId\\Content\\Facade"
           }
       }
   }
}